alias . 'cd ..'
alias cd 'cd !* && ll && setenv EMACS_VC pwd/dir.vc'
alias la 'ls -a -l'
alias ll 'ls -h -l'
alias lr 'ls -R'
alias lh 'df -h -a -T'
alias dh 'du -sh'
alias ds 'bsub -Is'
alias bi 'bsub -Is'
alias untracked 'git ls-files --others'
alias rtl_init 'cp /home/panweihao/script/Makefile ./ & cp /home/panweihao/script/dir.vc ../'
alias rs 'bsub -Is /tools/open/git-repo/repo sync'
#tagging
#alias repo_tag 'bsub -Is /tools/open/git-repo/repo sync --force-sync; tools/open/git-repo/repo forall -c "git tag -a -m \"1:2 \!:1\"; git push --tags'
alias repo_tag 'bsub -Is -q urgent /tools/open/git-repo/repo forall -c "git tag -a -m \"$(1) \!:1\"; git push --tags"'
alias repo_master 'bsub -Is -q urgent /tools/open/git-repo/repo forall -c "git checkout master";repo sync'
#co tag
alias repo_co_tag 'bsub -Is -q urgent /tools/open/git-repo/repo sync --force-sync; /tools/open/git-repo/repo forall -c pwd; git fetch --all; git checkout -b \\1; git pull'
alias repo_init 'bsub -Is /tools/open/git-repo/repo init -u ssh://git@git.aixin-chip.com/aixin.git -m \!:1 --repo-url /tools/open/git-repo'
alias repo_sta 'bsub -Is -q urgent /tools/open/git-repo/repo forall -c "pwd; git status"'
alias repo_sts 'bsub -Is -q urgent /tools/open/git-repo/repo forall -c "pwd; git status --porcelain"'
alias repo_forall 'bsub -Is -q urgent /tools/open/git-repo/repo forall -c "*"'
alias pdf 'FoxitReader'
